class Problem_1_MaxConsecutiveOnes {
    public int findMaxConsecutiveOnes(int[] nums) {
        int c=0;
        int res=0;
        for (int i : nums)
        {
            if(i==1){
                c=c+1;
                if(res<c)
                {
                    res=c;
                }
            }
            else
                c=0;
        
        }
        return res;
}
}
