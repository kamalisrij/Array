class Solution {
    public int[] replaceElements(int[] arr) {
        int m=arr[arr.length-1];
        arr[arr.length-1]=-1;
        for(int i=arr.length-2;i>-1;i--)
        {
            int c=arr[i];
            arr[i]=m;
            m=Math.max(c,m);
        }
        return arr;
        
    }
}
