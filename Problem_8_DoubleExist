class Solution {
    public boolean checkIfExist(int[] arr) {
        Set<Integer> s=new HashSet();
        for(int i:arr)
        {
            if((i%2)==0 && s.contains(i/2) || s.contains(2*i))
                return true;
                s.add(i);
        }
        
                return false;
            
    }
}
